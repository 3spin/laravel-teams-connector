{
    "name": "sebbmeyer/laravel-teams-connector",
    "description": "Microsoft Teams Connector for Laravel 5",
    "keywords": ["laravel","teams", "microsoft", "connectors", "connector", "messagecard"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Meyer",
            "email": "sebbmyr@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.x|5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x"
    },
    "autoload": {
        "psr-4": {
